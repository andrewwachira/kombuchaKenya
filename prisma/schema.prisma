generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model blogs {
  blog_id    Int     @id @default(autoincrement())
  blog_title String? @db.VarChar(255)
  blog_post  String? @db.VarChar(10485760)
  blog_image String? @db.VarChar(255)
}

model products {
  productid        Int  @id @default(autoincrement())
  flavor_name      String? @db.VarChar(255)
  flavor_type      String? @db.VarChar(255)
  product_category String? @db.VarChar(255)
  product_image    String?
  product_read     String?
}
